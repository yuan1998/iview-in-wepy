
    <i-cell-group>
        <i-cell title="基本用法">
            <i-switch value="{{switch1}}" bindchange="change" slot="footer"/>
        </i-cell>
        <i-cell title="自定义内容">
            <i-switch value="{{switch1}}" size="large" bindchange="change" slot="footer">
                <view slot="open">开启</view>
                <view slot="close">关闭</view>
            </i-switch>
        </i-cell>
        <i-cell title="图标">
            <i-switch value="{{switch1}}" bindchange="change" slot="footer">
                <i-icon type="right" slot="open"/>
                <i-icon type="close" slot="close"/>
            </i-switch>
        </i-cell>
        <i-cell title="禁止切换">
            <i-switch value="{{switch1}}" disabled="{{true}}" slot="footer"/>
        </i-cell>
    </i-cell-group>